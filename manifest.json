{
  "manifest_version": 3,
  "name": "Job Application Autofill",
  "version": "0.0.1",
  "description": "Autofill job application forms with your personal information.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
